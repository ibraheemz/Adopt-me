,{"generated":{"line":105,"column":73},"source":"Details.js","original":{"line":82,"column":8}},{"generated":{"line":105,"column":94},"source":"Details.js","original":{"line":83,"column":12}},{"generated":{"line":105,"column":123},"source":"Details.js","original":{"line":83,"column":13}},{"generated":{"line":105,"column":130},"source":"Details.js","original":{"line":83,"column":12}},{"name":"props","generated":{"line":105,"column":132},"source":"Details.js","original":{"line":83,"column":25}},{"generated":{"line":105,"column":137},"source":"Details.js","original":{"line":83,"column":12}},{"generated":{"line":105,"column":138},"source":"Details.js","original":{"line":82,"column":8}},{"generated":{"line":105,"column":139},"source":"Details.js","original":{"line":81,"column":4}},{"generated":{"line":106,"column":0},"source":"Details.js","original":{"line":86,"column":1}},{"generated":{"line":108,"column":0},"source":"Details.js","original":{"line":86,"column":1}}],"sources":{"Details.js":"import React from \"react\";\r\nimport pet from \"@frontendmasters/pet\";\r\nimport Carousel from \"./Carousel\";\r\nimport ErrorBoundary from \"./ErrorBoundary\";\r\nimport ThemeContext from \"./ThemeContext\";\r\nimport { navigate } from \"@reach/router\"\r\nimport  Modal  from \"./Modal\";\r\nclass Details extends React.Component {\r\n    state = {\r\n        loading: true,\r\n        showModal: false\r\n    }\r\n\r\n    componentDidMount() {\r\n        \r\n        pet.animal(this.props.id).then(({ animal }) => {\r\n            const { name, type, contact, description, photos, breeds } = animal\r\n            const { address } = contact\r\n            this.setState({\r\n                url: animal.url,\r\n                name: name,\r\n                animal: type,\r\n                location: `${address.city}, ${address.state}`,\r\n                description: description,\r\n                media: photos,\r\n                breed: breeds.primary,\r\n                loading: false\r\n            })\r\n        }, console.error)\r\n\r\n    }\r\n    toggleModal = () => this.setState({ showModal: !this.state.showModal })\r\n    adopt = () => navigate(this.state.url)\r\n    render() {\r\n        if(this.state.loading) {\r\n            return <h1>loading ...</h1>\r\n        } \r\n          const {name, animal, location, description, breed, media, showModal} = this.state;\r\n\r\n          return (\r\n              <div className=\"details\">\r\n                  \r\n                  <div>\r\n                    <Carousel media={media} />\r\n                    <h1>{name}</h1>\r\n                    <h2>{`${animal} - ${breed} - ${location}`}</h2>\r\n                    <ThemeContext.Consumer>\r\n                        {([theme]) => (\r\n                            <button \r\n                            style={{ backgroundColor: theme }}\r\n                            onClick={this.toggleModal}\r\n                            >Adopt {name}</button>\r\n                            \r\n                        )}\r\n                    </ThemeContext.Consumer>\r\n                    <p>{description}</p>\r\n\r\n                    {\r\n                        showModal ? (\r\n                            <Modal>\r\n                                <div>\r\n                                    <h1>Would you like to adopt {name}?</h1>\r\n                                    <div className=\"buttons\">\r\n                                        <button onClick={this.adopt}>Yes</button>\r\n                                        <button onClick={this.toggleModal}>No</button>\r\n                                    </div>\r\n                                </div>\r\n                            </Modal>\r\n                        ) : null\r\n                    }\r\n                    \r\n\r\n                  </div>\r\n              </div>\r\n          )\r\n    }\r\n\r\n}\r\n\r\nexport default function DetailsWithErrorBoundary(props) {\r\n    return (\r\n        <ErrorBoundary>\r\n            <Details {...props} />\r\n        </ErrorBoundary>\r\n    )\r\n};\r\n"},"lineCount":null}},"error":null,"hash":"4b3b59fcbe593dc6283116fa68a5dcde","cacheData":{"env":{}}}